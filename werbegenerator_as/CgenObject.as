package  {		public class CgenObject {		//--------------------------------------//  PRIVATE VARIABLES//--------------------------------------		private var _clipname:String;		private var _beauty:int;		private var _price:int; 		private var _pleasure:int;		private var _freshness:int;		private var _safety:int;		private var _joy:int;		private var _quality:int;		private var _freedom:int;		private var _innovation:int;		private var _luxury:int;				private var _beginning:String;		private var _middle:String;		private var _end:String;		private var _media:String;		private var _emotionality:int;		private var _motivating:int;		private var _size:int;		private var _sensual:int;		private var _happy:int;		private var _modern:int;//--------------------------------------//  CONSTRUCTOR//--------------------------------------		public function CgenObject(theClipname:String,								   theBeauty:int,								   thePrice:int,								   thePleasure:int,								   theFreshness:int,							  								   theSafety:int,								   theJoy:int,								   theQuality:int,								   theFreedom:int,								   theInnovation:int,								   theLuxury:int,								   								   theBeginning:String,								   theMiddle:String,								   theEnd:String,								   								   theMedia:String,								   								   theEmotionality:int,								   theMotivating:int,								   theSize:int,								   theSensual:int,								   theHappy:int,								   theModern:int								   ) 		{						_clipname = theClipname						_beauty = theBeauty;			_price = thePrice;			_pleasure = thePleasure;			_freshness = theFreshness;			_safety = theSafety;			_joy = theJoy;			_quality = theQuality;			_freedom = theFreedom;			_innovation = theInnovation;			_luxury = theLuxury;						_beginning = theBeginning;			_middle = theMiddle;			_end = theEnd;						_media = theMedia;						_emotionality = theEmotionality;			_motivating = theMotivating;			_size = theSize;			_sensual = theSensual;			_happy = theHappy;			_modern = theModern;		}		//--------------------------------------//  GETTERS//--------------------------------------				public function get getClipName():String{			return _clipname;		}				public function get getMedia():String{			return _media;		}				public function get getBeauty():int{			return _beauty;		}				public function get getPrice():int{			return _price;		}				public function get getPleasure():int{			return _pleasure;		}				public function get getFreshness():int{			return _freshness;		}				public function get getSafety():int{			return _safety;		}				public function get getJoy():int{			return _joy;		}				public function get getQuality():int{			return _quality;		}				public function get getFreedom():int{			return _freedom;		}				public function get getInnovation():int{			return _innovation;		}				public function get getLuxury():int{			return _luxury;		}				public function get getBeginning():String{			return _beginning;		}				public function get getMiddle():String{			return _middle;		}				public function get getEnd():String{			return _end;		}				public function get getEmotionality():int{			return _emotionality;		}				public function get getMotivating():int{			return _motivating;		}				public function get getSize():int{			return _size;		}				public function get getSensual():int{			return _sensual;		}				public function get getHappy():int{			return _happy;		}				public function get getModern():int{			return _modern;		}		//--------------------------------------				public function getAllAttributes():Object		{			var hashMap:Object = {				beauty:this._beauty,				price:this._price,				pleasure:this._pleasure,				freshness:this._freshness,				safety:this._safety,				joy:this._joy,				quality:this._quality,				freedom:this._freedom,				innovation:this._innovation,				luxury:this._luxury			}			return hashMap;		}		//--------------------------------------				public function getAllMoods():Object		{			var hashMap:Object = {				emotionality:this._emotionality,				motivating:this._motivating,				size:this._size,				sensual:this._sensual,				modern:this._modern			}			return hashMap;		}		//--------------------------------------	public function getPos():String	{		var position:String;				if(this._beginning == "true")			return "beginning";		if(this._middle == "true")			return "middle";		if(this._end == "true")			return "end";					return position;	}	//--------------------------------------			//trace(key + " : " + testHash[key]);			//if(testHash == hashMap)trace("HASH MAP IS SAME");			//else trace("NOPE");			/*			for(var key:Object in filterHash) 			{				trace("CgenCalc: " + key + " " + filterHash[key]);			}			*/	}	}